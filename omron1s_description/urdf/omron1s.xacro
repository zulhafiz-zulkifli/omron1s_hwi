        <color
          rgba="0.792156862745098 0.819607843137255 0.933333333333333 1" />
      </material>
    </visual>
    <collision>
      <origin
        xyz="0 0 0"
        rpy="0 0 0" />
      <geometry>
        <mesh
          filename="file:///$(find omron1s_description)/meshes/link1.STL" />
      </geometry>
    </collision>
  </link>
  <joint
    name="joint1"
    type="revolute">
    <origin
      xyz="0.0587564520357434 -0.0846616099911215 0.151979361094108"
      rpy="1.5707963267949 0 -2.77072298333055" />
    <parent
      link="base_link" />
    <child
      link="link1" />
    <axis
      xyz="0 1 0" />
    <limit
      lower="-3.14"
      upper="3.14"
      effort="500"
      velocity="6.28" />
    <dynamics damping="60.0" friction="4.0" />
  </joint>
  <link
    name="link2">
    <inertial>
      <origin
        xyz="-0.095000000000001 -2.77555756156289E-16 0.275"
        rpy="0 0 0" />
      <mass
        value="2.45132741228718" />
      <inertia
        ixx="0.0781860308173814"
        ixy="0"
        ixz="4.32325616278595E-18"
        iyy="0.0774228597300263"
        iyz="7.23162849047831E-17"
        izz="0.00178455750914138" />
    </inertial>
    <visual>
      <origin
        xyz="0 0 0"
        rpy="0 0 0" />
      <geometry>
        <mesh
          filename="file:///$(find omron1s_description)/meshes/link2.STL" />
      </geometry>
      <material
        name="">
        <color
          rgba="0.792156862745098 0.819607843137255 0.933333333333333 1" />
      </material>
    </visual>
    <collision>
      <origin
        xyz="0 0 0"
        rpy="0 0 0" />
      <geometry>
        <mesh
          filename="file:///$(find omron1s_description)/meshes/link2.STL" />
      </geometry>
    </collision>
  </link>
  <joint
    name="joint2"
    type="revolute">
    <origin
      xyz="0 0.0400000000000031 -0.0999999999999985"
      rpy="1.97667516838365 0 3.14159265358979" />
    <parent
      link="link1" />
    <child
      link="link2" />
    <axis
      xyz="1 0 0" />
    <limit
      lower="-3.14"
      upper="3.14"
      effort="500"
      velocity="6.28" />
