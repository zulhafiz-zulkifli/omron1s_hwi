    </collision>
  </link>
  <joint
    name="joint4"
    type="revolute">
    <origin
      xyz="0 0 0.55"
      rpy="0.535953139821402 0 -3.14159265358979" />
    <parent
      link="link3" />
    <child
      link="link4" />
    <axis
      xyz="1 0 0" />
    <limit
      lower="-3.14"
      upper="3.14"
      effort="500"
      velocity="6.28" />
    <dynamics damping="60.0" friction="4.0" />

  </joint>

